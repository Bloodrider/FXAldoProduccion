			}
			if(tipoIndicador.selectedItem[0].toString() == 2)
			{
				graficasPN.title = "Monto de facturas emitidas por aplicación";
			}
			if(tipoIndicador.selectedItem[0].toString() == 3)
			{
				graficasPN.title = "Monto de Facturas Emitidas por Cliente";
			}
			if(tipoIndicador.selectedItem[0].toString() == 4)
			{
				graficasPN.title = "Facturas Emitidas por Aplicación por Día";
			}
			if(tipoIndicador.selectedItem[0].toString() == 5)
			{
				graficasPN.title = "Clientes Registrados por Aplicación por Día";
			}
			if(tipoIndicador.selectedItem[0].toString() == 6)
			{
				graficasPN.title = "Empresas por Estado";
			}
			if(tipoIndicador.selectedItem[0].toString() == 7)
			{
				graficasPN.title = "Facturas Emitidas por Día";
			}
			if(tipoIndicador.selectedItem[0].toString() == 8)
			{
				graficasPN.title = "Clientes Registrados por Día";
			}
			if(tipoIndicador.selectedItem[0].toString() == 9)
			{
				graficasPN.title = "Facturas por Aplicación";
			}
			if(tipoIndicador.selectedItem[0].toString() == 10)
			{
				graficasPN.title = "Empresas por Aplicación";
			}
			if(tipoIndicador.selectedItem[0].toString() == 11)
			{
				graficasPN.title = "Carga de LCO por día, si esta en uno la gráfica es que fue cargada en el día indicado.";
			}
			if(tipoIndicador.selectedItem[0].toString() == 11)
			{
				graficasPN.title = "Porcentaje de Incidencias en Envío SAT por Día, no debe de rebasar 0.1% mensual.";
			}
			if(tipoIndicador.selectedItem[0].toString() == 11)
