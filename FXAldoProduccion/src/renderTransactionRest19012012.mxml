<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" 
				autoDrawBackground="true" height="70" >
			
		<mx:VBox width="100%" height="100%" verticalGap="5">
			<mx:HBox width="100%" verticalAlign="middle">			
				<s:Label text="{data[1]}" fontWeight="bold" color="#27749C"/>
			</mx:HBox>

			<mx:HBox width="100%" verticalAlign="middle">			
				<mx:VBox width="100%" verticalAlign="middle">	
					<mx:HBox width="100%" verticalAlign="middle">	
						<mx:Spacer width="10"/>
						<s:Label text="{data[2]}" fontSize="10" fontWeight="normal"/>
					</mx:HBox>
					<mx:HBox width="100%" verticalAlign="middle">	
						<mx:Spacer width="10"/>
						<s:Label text="{data[3]}" fontSize="10" fontWeight="normal"/>
					</mx:HBox>
					<mx:HBox width="100%" verticalAlign="middle">	
						<mx:Spacer width="10"/>
						<s:Label text="{data[4]}" fontSize="10" fontWeight="normal"/>
					</mx:HBox>
				</mx:VBox>
				
				<mx:VBox width="30%" verticalAlign="middle">
					<mx:Image source="assets/2278-32.png" x="175" y="5"/>
				</mx:VBox>
			</mx:HBox>
			
			<mx:HRule height="1" width="100%">
			</mx:HRule>
		</mx:VBox>
	
</s:ItemRenderer>
