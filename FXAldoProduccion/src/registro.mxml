			private function validData1():void			
			{				
				
				var validarTab:String;	
				var mError:String = ""
				var controlsValid:Array = new Array();
				
				controlsValid.push(this.nombre,this.apePat,this.apeMat,this.email,
					this.emailConfirm,this,telefono,this.nombreNegocio,this.giroNegocio,
					this.numEmpleados,this.checkInput);
				
				if(chkUserCode.selected == true)
				{
					controlsValid.push(this.userCode);
				}
				
				validarTab = validar(controlsValid);
				
				if(validarTab == "1")
				{
					if (validUser.text > "0")
					{
						mError+="La cuenta ingresada ya existe en el sistema, intente con otra opción.\n";
						email.text = "";
						emailConfirm.text = "";
					}
					if(validNextMAIL.text == "0")
					{
						mError+="EL Formato de Correo Electrónico es incorrecto.\n";
					}
					if(validNextMAILC.text == "0")
					{
						mError+="EL Formato de la Confirmación de Correo Electrónico es incorrecto.\n";
					}
					if(email.text.length > 0 && emailConfirm.text.length > 0)
					{
						if(email.text != emailConfirm.text)
						{		
							mError+="El correo electrónico y su confirmación deben coincidir.\n";
						}
					}
					if(email.text.length<8)
					{
						mError+="La cuenta debe tener mínimo 8 caracteres.\n";
					}
					if(telefono.text.length != 10)
					{		
						mError+= "El número de teléfono debe ser de 10 dígitos.\n";
					}
					if (captchaCode != checkInput.text)
					{					
						mError+="El código de la imagen no corresponde al ingresado.\n";
						remakeCaptcha();	
					}
					
					if(chkUserCode.selected)
					{
						if (userCode.text.length != 40) 
						{					
							mError+="El código de usuario deberá contener 40 caracteres.\n";	
						}
						
						if (isUserCodeValid.text == "0")
						{
							mError+= "El Código de Usuario ingresado es incorrecto. Favor de verificar.\n";
						}
						else if (isUserCodeValid.text == "2")
						{
							mError+= "El Código de Usuario ingresado ya está asignado a otra cuenta. Favor de verificar\n";
						}
						else if (isUserCodeValid.text == "3")
						{
							mError+= "El Código de Usuario ingresado ya está dado de baja. Favor de verificar.\n";
						}
					}
					
					if(mError.length > 0)
					{
						Alert.cancelLabel="Cancelar";
						Alert.okLabel = "Aceptar";
						Alert.show(mError, "Aviso", Alert.OK, null, null, iconNot);
						mError="";
						return;	
					}
					else
					{
						//Send(662);//VALIDA SI ES UN CODIGO DE USUARIO VALIDO
						btnFlujo(2);
					}
				}
			}
