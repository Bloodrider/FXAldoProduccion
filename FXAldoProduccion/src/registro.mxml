			private function guardarRegistro():void
			{
				var mError:String = ""
				
				if(!TyC.selected)
				{
					mError+="Acepte los Términos y Condiciones de uso del Servicio.\n";
				}
				if(mError.length > 0)
				{
					Alert.cancelLabel="Cancelar";
					Alert.okLabel = "Aceptar";
					Alert.show(mError, "Aviso", Alert.OK, null, null, iconNot);
					mError="";
					return;	
				}
				else
				{
					Send(623);//GUARDAR SOLICITUD	
				}
			}
			
			private function viewMSG():void
			{
				if(resultMsg.text.length > 0)
				{
					lblResultMsg.text = resultMsg.text;
					msgSolicitud.visible = true;
				}
			}
			
			private function btnFlujo(OP:int):void
			{        	
				if(OP == 1)
				{
					capturaDatos.visible = true;
					verificarDatos.visible =false;
					comprobanteSolicitud.visible =false;
					
					paso1.alpha = 1;
					paso2.alpha = 0.3;
					paso3.alpha = 0.3;
					
					imgPaso1.visible = true;
					imgPaso2.visible = true;
					imgPaso3.visible = true;
					
				}
				else if(OP == 2)
				{
					capturaDatos.visible = false;
					verificarDatos.visible =true;
					comprobanteSolicitud.visible =false;
					
					paso1.alpha = 1;
					paso2.alpha = 1;
					paso3.alpha = 0.3;
					
					imgPaso1.visible = true;
					imgPaso2.visible = true;
					imgPaso3.visible = true;
				}
				else if(OP == 3)
				{
					if(result.text != "0")
					{
						capturaDatos.visible = false;
						verificarDatos.visible =false;
						comprobanteSolicitud.visible =true;
						
						paso1.alpha = 1;
						paso2.alpha = 1;
						paso3.alpha = 1;
						
						imgPaso1.visible = true;
						imgPaso2.visible = true;
						imgPaso3.visible = true;
					}
					else if(result.text == "0")
					{
						btnFlujo(1);
					}
				}
				else if(OP == 4)
				{
					if(result.text != "0")
					{
						var urlRequest:URLRequest = new URLRequest("https://fx.facturaxion.net/fx.html");
						navigateToURL(urlRequest,"_self");
						
					}
					else if(result.text == "0")
					{
						result.text = "";
						lblResultMsg.text = ""; 
						resultMsg.text = "";
						msgSolicitud.visible = false;
						btnFlujo(1);
					}
				}
			}
			
